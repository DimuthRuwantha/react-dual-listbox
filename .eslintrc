{
  "extends": "airbnb",
  "parser": "babel-eslint",
  "rules": {
    "indent": [2, "tab"],
    "class-methods-use-this": 0,
    "no-tabs": 0,
    "react/jsx-indent" : [2, "tab"],
    "react/jsx-indent-props" : [2, "tab"],
    "react/jsx-filename-extension": 0,
    "react/sort-prop-types": [2, {
      "callbacksLast": true,
      "requiredFirst": true
    }]
  }
}
